generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  // THIS WAS MISSING VVV
  url      = env("DATABASE_URL")
}

model WaitlistApplication {
  id        String   @id @default(cuid())
  email     String   @unique
  firstName String
  lastName  String
  linkedIn  String?
  dealSize  String?
  icp       String?
  createdAt DateTime @default(now())
  status    String   @default("PENDING") 
}